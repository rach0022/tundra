{
  "name": "com.algonquinlive.rach0022.tundra",
  "displayName": "Tundra",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "ios-test": "cordova run ios --target=\"iPhone-8, 13.3\"",
    "rm-platforms": "cordova platform rm android && cordova platform rm ios",
    "add-platforms": "cordova platform add android && cordova platform add ios"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Ravi Rachamalla <rach0022@algonquinlive.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-splashscreen": "^5.0.3"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-device": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rach0022/tundra.git"
  },
  "bugs": {
    "url": "https://github.com/rach0022/tundra/issues"
  },
  "homepage": "https://github.com/rach0022/tundra#readme"
}